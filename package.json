{
	"name": "snes-studio",
	"displayName": "SNES Development Studio",
	"publisher": "bitwiseMobile-games",
	"description": "An extension to help make developing snes applications easier.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"icon": "images/icon.png",
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"keywords": [
		"6502",
		"assembler",
		"assembly",
		"Z80",
		"SNES"
	],
	"activationEvents": [
		"*",
		"onCommand:extension.openWelcomePage",
		"onCommand:snes-studio.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "snes-studio.helloWorld",
				"title": "Hello World"
			}
		],
		"languages": [
			{
				"id": "wla-dx-asm",
				"aliases": [
					"WLA-DX",
					"wla-dx-asm"
				],
				"extensions": [
					".wla",
					".asm",
					".s",
					".a",
					".i"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "wla-dx-asm",
				"scopeName": "source.wla-dx-asm",
				"path": "./syntaxes/wla-dx-asm.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "wla-dx-asm",
				"path": "./snippets.json"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/soshimozi/snes-dev-studio.git"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"open": "^7.0.4"
	}
}
